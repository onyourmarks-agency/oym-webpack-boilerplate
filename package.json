{
	"name": "oym-webpack-boilerplate",
	"version": "6.1.0",
	"main": "app.ts",
	"license": "MIT",
	"config": {
		"node": "20"
	},
	"scripts": {
		"watch": "node webpack/pre-script.js && webpack --watch --config webpack/webpack.config.ts --stats-children",
		"pre": "node webpack/pre-script.js",
		"build": "node  webpack/pre-script.js && NODE_ENV=prod webpack --mode production --config webpack/webpack.config.ts --stats-children",
		"frontend-cs": "pnpm format && pnpm eslint && pnpm stylelint && pnpm svelte-check",
		"lint": "pnpm prettier --cache --check . && pnpm eslint && pnpm stylelint",
		"eslint": "node webpack/pre-script.js && eslint assets --cache --ext .js,.ts,.svelte",
		"eslint-fix": "node webpack/pre-script.js && eslint assets --cache --fix --ext .js,.ts,.svelte",
		"format": "node webpack/pre-script.js && prettier --cache --write .",
		"stylelint": "stylelint -f verbose --cache 'assets/**/*.{css,scss,svelte}'",
		"stylelint-fix": "stylelint -f verbose --cache 'assets/**/*.{css,scss,svelte}' --fix",
		"svelte-check": "svelte-check"
	},
	"dependencies": {
		"@onyourmarks/outdated-browser-rework": "^3.0.0",
		"bazinga-translator": "^6.0.0",
		"formbouncerjs": "^1.4.6",
		"jump.js": "^1.0.2",
		"normalize.css": "^8.0.1",
		"stylelint-config-recommended": "^14.0.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-scss": "^6.2.0",
		"stylelint-selector-bem-pattern": "^4.0.0",
		"svelte": "^4.2.12",
		"swiper": "^11.0.7"
	},
	"devDependencies": {
		"@babel/cli": "^7.23.9",
		"@babel/core": "^7.24.0",
		"@babel/eslint-parser": "^7.23.10",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.24.0",
		"@babel/preset-env": "^7.24.0",
		"@babel/preset-typescript": "^7.23.3",
		"@babel/runtime": "^7.24.0",
		"@nuxt/friendly-errors-webpack-plugin": "^2.6.0",
		"@onyourmarks/webpack-svg-plugin": "^4.0.0",
		"@tsconfig/node20": "^20.1.2",
		"@tsconfig/svelte": "^5.0.2",
		"@types/bazinga-translator": "^3.0.2",
		"@types/jump.js": "^1.0.6",
		"@types/lodash": "^4.14.202",
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"@typescript-eslint/parser": "^7.1.0",
		"autoprefixer": "^10.4.18",
		"babel-loader": "^9.1.3",
		"browserslist": "^4.23.0",
		"check-node-version": "^4.2.1",
		"clean-webpack-plugin": "^4.0.0",
		"core-js": "^3.36.0",
		"css-loader": "^6.10.0",
		"css-minimizer-webpack-plugin": "^6.0.0",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-webpack": "^0.13.8",
		"eslint-plugin-compat": "^4.2.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-webpack-plugin": "^4.0.1",
		"fsevents": "^2.3.3",
		"html-webpack-plugin": "^5.6.0",
		"intl-messageformat": "^10.5.11",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^2.8.1",
		"postcss": "^8.4.35",
		"postcss-html": "^1.6.0",
		"postcss-loader": "^8.1.1",
		"prettier": "^3.2.5",
		"prettier-plugin-css-order": "^2.0.1",
		"prettier-plugin-svelte": "^3.2.2",
		"sass": "^1.71.1",
		"sass-loader": "^14.1.1",
		"stylelint": "^16.2.1",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-standard-scss": "^13.0.0",
		"svelte-check": "^3.6.6",
		"svelte-check-plugin": "^1.0.4",
		"svelte-loader": "^3.1.9",
		"svelte-preprocess": "^5.1.3",
