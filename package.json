{
	"name": "tde-webpack-boilerplate",
	"version": "5.1.0",
	"main": "app.ts",
	"license": "MIT",
	"config": {
		"node": "18"
	},
	"scripts": {
		"watch": "node webpack/pre-script.js && webpack --watch --config webpack/webpack.config.js --stats-children",
		"pre": "node webpack/pre-script.js",
		"build": "node webpack/pre-script.js && NODE_ENV=prod webpack --mode production --config webpack/webpack.config.js --stats-children",
		"frontend-cs": "pnpm format && pnpm eslint && pnpm stylelint && pnpm svelte-check",
		"lint": "pnpm prettier --cache --check . && pnpm eslint && pnpm stylelint",
		"eslint": "node webpack/pre-script.js && eslint assets --cache --ext .js,.ts,.svelte",
		"eslint-fix": "node webpack/pre-script.js && eslint assets --cache --fix --ext .js,.ts,.svelte",
		"format": "node webpack/pre-script.js && prettier --cache --write .",
		"stylelint": "stylelint -f verbose --cache 'assets/**/*.{css,scss,svelte}'",
		"stylelint-fix": "stylelint -f verbose --cache 'assets/**/*.{css,scss,svelte}' --fix",
		"svelte-check": "svelte-check"
	},
	"dependencies": {
		"bazinga-translator": "^5.0.0",
		"formbouncerjs": "^1.4.6",
		"jump.js": "^1.0.2",
		"normalize.css": "^8.0.1",
		"outdated-browser-rework": "github:tdeNL/outdated-browser-rework",
		"stylelint-config-recommended": "^13.0.0",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-selector-bem-pattern": "^3.0.1",
		"svelte": "^3.56.0",
		"swiper": "^8.4.7"
	},
	"devDependencies": {
		"@babel/cli": "^7.23.0",
		"@babel/core": "^7.23.2",
		"@babel/eslint-parser": "^7.22.15",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.23.2",
		"@babel/preset-env": "^7.23.2",
		"@babel/preset-typescript": "^7.23.2",
		"@babel/runtime": "^7.23.2",
		"@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
		"@tsconfig/svelte": "^3.0.0",
		"@types/jump.js": "^1.0.5",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"autoprefixer": "^10.4.16",
		"babel-loader": "^9.1.3",
		"browserslist": "^4.22.1",
		"check-node-version": "^4.2.1",
		"clean-webpack-plugin": "^4.0.0",
		"core-js": "^3.33.1",
		"css-loader": "^6.8.1",
		"css-minimizer-webpack-plugin": "^5.0.1",
		"eslint": "^8.51.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-import-resolver-webpack": "^0.13.7",
		"eslint-plugin-compat": "^4.2.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"eslint-webpack-plugin": "^4.0.1",
		"fsevents": "^2.3.3",
		"html-webpack-plugin": "^5.5.3",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^2.7.6",
		"postcss": "^8.4.31",
		"postcss-html": "^1.5.0",
		"postcss-loader": "^7.3.3",
		"prettier": "^3.0.3",
		"prettier-plugin-css-order": "^2.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"sass": "^1.69.4",
		"sass-loader": "^13.3.2",
		"stylelint": "^15.11.0",
		"stylelint-config-html": "^1.1.0",
		"svelte-check": "^3.1.0",
		"svelte-check-plugin": "^1.0.4",
		"svelte-loader": "^3.1.7",
		"svelte-preprocess": "^4.10.7",
		"svelte-preprocess-sass": "^2.0.1",
		"svg-sprite-loader": "^6.0.11",
		"tde-webpack-svg-plugin": "^3.0.0",
		"terser-webpack-plugin": "^5.3.9",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.5",
		"webpack": "^5.76.0",
		"webpack-cli": "^4.10.0",
		"webpack-extraneous-file-cleanup-plugin": "^2.0.0",
		"webpack-manifest-plugin": "^5.0.0",
		"webpack-notifier": "^1.15.0",
		"webpack-retry-chunk-load-plugin": "^3.1.1"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"postcss": "5.2.18"
			}
		}
	},
	"browserslist": {
		"production": [
			"> 1%",
			"not dead",
			"not ie <= 11",
			"not op_mini all"
		],
		"modern": [
			"last 1 version"
		]
	}
}
