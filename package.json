{
  "name": "tde-webpack-boilerplate",
  "version": "3.0.0",
  "main": "app.js",
  "license": "MIT",
  "config": {
    "node": "16"
  },
  "scripts": {
      "watch": "node webpack/pre-script.js && webpack --watch --config webpack/webpack.config.js --stats-children",
      "build": "node webpack/pre-script.js && NODE_ENV=prod webpack --mode production --config webpack/webpack.config.js --stats-children",
      "lint": "node webpack/pre-script.js && prettier --check . --plugin-search-dir=. && eslint assets --ext .js,.svelte --ignore-path .gitignore",
      "lint-fix": "node webpack/pre-script.js && eslint assets --fix --ext .js,.svelte --ignore-path .gitignore",
      "lint-fix-nocheck": "node webpack/pre-script.js && prettier --check . --plugin-search-dir=. && eslint assets --fix --ext .js,.svelte --ignore-path .gitignore",
      "format": "node webpack/pre-script.js && prettier --write . --plugin-search-dir=.",
      "format-az": "node webpack/pre-script.js && ORDER=alphabetical prettier --write . --plugin-search-dir=",
      "eslint": "pnpm run lint",
      "stylelint": "stylelint -f verbose 'assets/css/**/*.{css,scss}'",
      "stylelint-fix": "stylelint -f verbose 'assets/css/**/*.{css,scss}' --fix",
      "frontend-check": "pnpm format && pnpm lint-fix-nocheck && pnpm stylelint"
  },
  "dependencies": {
    "bazinga-translator": "^3.0.1",
    "bourbon": "^7.0.0",
    "bourbon-neat": "^4.0.0",
    "formbouncerjs": "^1.4.6",
    "normalize.css": "^8.0.1",
    "outdated-browser-rework": "git+https://github.com/tdeNL/outdated-browser-rework.git",
    "svgxuse": "^1.2.6",
    "tiny-slider": "^2.9.2",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.0",
    "@considonet/stylelint-config-bem": "^1.0.1",
    "@considonet/stylelint-config-scss": "^1.0.1",
    "@e-sites/eslint-config": "^2.0.0",
    "@e-sites/stylelint-config-esites": "^1.0.1",
    "autoprefixer": "^10.4.0",
    "@babel/eslint-parser": "^7.16.0",
    "babel-loader": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.17.6",
    "check-node-version": "^4.2.1",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.2",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-webpack-plugin": "^3.1.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-import": "^2.25.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.4",
    "node-sass": "^6.0.1",
    "picturefill": "^3.0.3",
    "postcss-loader": "^6.2.0",
    "prettier": "2.3.0",
    "prettier-plugin-css-order": "^1.0.0",
    "prettier-plugin-svelte": "^2.4.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "svg-sprite-loader": "^6.0.11",
    "tde-webpack-mjml-plugin": "^2.0.0",
    "tde-webpack-svg-plugin": "^2.0.0",
    "webpack": "^5.62.1",
    "webpack-cli": "^4.9.1",
    "webpack-extraneous-file-cleanup-plugin": "^2.0.0",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-notifier": "^1.14.1"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not ie <= 10",
      "not op_mini all"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
