{
	"assets/**/*.{css,scss,svelte}": [
		"node webpack/pre-script.js",
		"prettier --cache --check --write .",
		"stylelint -f verbose --cache"
	],
	"assets/**/*.{ts,svelte}": [
		"node webpack/pre-script.js",
		"prettier --cache --check --write .",
		"eslint --cache",
		"svelte-check --no-tsconfig",
		"tsc --noEmit"
	]
}
